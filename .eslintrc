{
    "parser": "babel-eslint",
    "rules": {
        "max-len": [1, 70, 2, {"ignoreComments": true}],
        "prop-types": [0], 
        "eol-last": [0], 
        "comma-dangle": [0],
        "no-undef":[0],
        "no-unused-vars": [0],
        "semi": [0],
        "indent": ["error", 4],
        "import/prefer-default-export": [0],
        "import/no-named-as-default": [0],
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }, 
    "extends": ["airbnb-base"]
}